<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="s07150750.gdemec.edu.cn.mycamera">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CameraActivity" android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AlbumActivity" android:screenOrientation="landscape"></activity>
    </application>

    <user-permission android:name="android.permission.CAMERA"/>
    <user-feature android:name="android.hardware.camera.autofocus"/>
    <user-feature android:name="android.hardware.camera"/>
    <user-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <user-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
</manifest>